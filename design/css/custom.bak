@charset "utf-8";
/* CSS Document */

/* ==================================== GENERAL CSS STYLES ==================================== */

@font-face {
	font-family: 'Roboto-Regular';
	src: url('fonts/Roboto-Regular.eot');
	src: url('fonts/Roboto-Regular.eot?#iefix') format('embedded-opentype'), url('fonts/Roboto-Regular.woff') format('woff'), url('fonts/Roboto-Regular.ttf')    format('truetype'), url('fonts/Roboto-Regular.svg#aachen_btroman') format('svg');
	font-weight: light;
	font-style: light;
}
@font-face {
	font-family: 'Roboto-Light';
	src: url('fonts/Roboto-Light.eot');
	src: url('fonts/Roboto-Light.eot?#iefix') format('embedded-opentype'), url('fonts/Roboto-Light.woff') format('woff'), url('fonts/Roboto-Light.ttf')    format('truetype'), url('fonts/Roboto-Light.svg#aachen_btroman') format('svg');
	font-weight: light;
	font-style: light;
}
@font-face {
	font-family: 'Calibri-Light';
	src: url('fonts/Calibri-Light.eot');
	src: url('fonts/Calibri-Light.eot?#iefix') format('embedded-opentype'), url('fonts/Calibri-Light.woff') format('woff'), url('fonts/Calibri-Light.ttf')    format('truetype'), url('fonts/Calibri-Light.svg#aachen_btroman') format('svg');
	font-weight: light;
	font-style: light;
}
html,
body {
    overflow-x: hidden;
    font-family: 'Calibri-Light';
}
body {
    margin: 0px;
    background: #ffffff;
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    color: #414141;
}
body,
input,
select,
textarea {
    font: 14px 'Roboto', Arial, Helvetica, sans-serif;
    line-height: 22px;
    color: #000;
}
h1,
h2,
h3,
h4,
h5,
h6,
ul,
li {
    margin: 0px;
    padding: 0px;
}
ul,
ol {
    list-style: none;
}
input {
    border: 0 none;
}
a:focus,
a:visited,
a:active,
input:active,
input:focus,
select:focus,
img:focus,
img,
area,
map,
*:focus {
    outline: none;
    outline-width: 0px;
    ie-dummy: expression(this.hideFocus=true);
}
a,
a:focus,
a:hover {
    text-decoration: none;
}
.top-ad-box {
	float: none;
	width: 846px !important;
        height: 120px;
        overflow: hidden;
	margin:0 auto;
	padding: 0;
	background: url(../img/ad-bg-img.png) repeat 0 0;
	font-size: 30px;
	font-weight: 600;
	text-align: center;
}

/*=======GlobaL Styling==========*/

header {
    float: left;
    width: 100%;
}
.modal-open {
    padding-right: 0px !important;
}
.glb-wrap {
    padding: 30px 0px;
}
.navbar {
    margin-bottom: 10px;
}
.navbar-logo {
	float: left;
	width: 275px;
	height:50px;
	margin-top: 27px;
}

.navbar-default {
	border: 0px;
}

.navbar-default {
	background-color: #ffffff;
	border-color: #ffffff;
}

.navbar-nav {
	margin-top: 37px;
}
.navbar-default .navbar-nav>li>a,
.navbar-default .navbar-nav>.open>a {
    font-family: verdana;
    color: #2965be;
    font-weight: 700;
    font-size: 17px;
    padding: 0px 15px;
    margin-top: 10px;
}
.navbar-default .navbar-nav>li>a:hover,
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover {
    color: #a4b502;
}
.navbar-default .navbar-nav>li>a:focus,
.navbar-default .navbar-nav>li>a:hover {
    color: #a4b502;
}
.navbar-default .navbar-nav>.open>a:focus {
	background-color: #fff;
}
.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:focus, .navbar-default .navbar-nav > .open > a:hover {
    background-color: none !important;
    color: #555;
}
.navbar-default .navbar-nav>.open>a,
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover {
    color: #555;
    background-color: #fff !important;
}

.navbar-default .navbar-nav>li>a>i {
	margin-right: 10px;
	font-size: 22px
}

.navbar-default .navbar-nav>li>a.btn-submit-ad {
	color: #fff;
    font-size: 17px;
	line-height: 25px;
	padding: 8px 20px;
	margin-top: 0px;
}
.navbar-nav>li>a.btn-submit-ad:hover,
.navbar-nav>li>a.btn-submit-ad:focus {
    /*background-color:#ffa454;*/
    
    background-color: #a4b502;
}
.navbar-right {
    margin-right: 0;
}
