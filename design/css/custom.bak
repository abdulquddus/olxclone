}
.regions-col ul {
    padding: 10px 10px 10px 0;
}
.regions-col ul li a {
    font-size: 16px;
    line-height: 26px;
}
.regions-col ul li a:hover {
    color: #a4b502;
}
/*=====--Category---=====*/

.category-wrap {
    float: left;
    width: 100%;
    border: 1px solid #ededed;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
.category-left-list {
    padding: 15px 0px 35px;
    background-color: #f8f8f8;
}
.category-left-list h3 {
    font-size: 15px;
    line-height: 18px;
    padding: 9px 20px;
    font-weight: 400;
}
.category-left-list ul {
    padding: 0px 0px 15px 0px;
    font-size: 15px;
    line-height: 18px;
}
.category-left-list ul li {
    position: relative;
    margin: 0px 20px;
}
.category-left-list ul li a {
    display: block;
    color: #2965be;
    padding: 9px 0px;
    font-size:16px;
}
.category-left-list ul li a:hover {
    color: #a4b502;
}
.category-left-list ul li:first-child {
    border-bottom: 1px solid #e6e6e6;
    -webkit-box-shadow: 0px 1px 0px 0px rgba(255, 255, 255, 1);
    -moz-box-shadow: 0px 1px 0px 0px rgba(255, 255, 255, 1);
    box-shadow: 0px 1px 0px 0px rgba(255, 255, 255, 1);
    margin-bottom: 7px;
}
.category-left-list ul li:first-child a {
    padding-top: 0px;
}
.category-left-list ul li.active {
    background: #ffffff;
    border: 1px solid #e1e1e1;
    border-right: 0px;
    color: #000;
    -webkit-border-radius: 5px 0px 0px 5px;
    border-radius: 5px 0px 0px 5px;
    margin: 0 0 0 -10px;
    padding-left: 27px;
}
.category-left-list ul li.active:after {
    position: absolute;
    right: -15px;
    top: -1px;
    content: ' ';
    width: 17px;
    height: 38px;
    background: url(../img/cat-arw-act.png) no-repeat;
}
.category-main {
    padding: 15px;
}
.changeloc-box {
    background-color: #FBFAAE;
    padding: 12px 15px;
    margin-left: 12px;
    width: 98%;
}
.changeloc-box-inr {
    font-size: 17px;
    line-height: 20px;
    color: #336DBE;
    color: #000;
    font-weight: bold;
}
.changeloc-box-inr i {
    font-size: 22px;
    margin-right: 10px;
}
.changeloc-box-inr a {
    color: #336DBE;
}
.changeloc-box-inr a:hover {
    color: #a4b502;
}
.changeloc-box-inr span {
    color: #414141;
    font-weight: 400;
    font-size: 12px;
    padding-left: 5px;
}
