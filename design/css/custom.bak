/*=====-- Location Box Popup ---=====*/

.location-modal {} .location-modal .modal-dialog {
    width: 800px;
    margin-top: 300px;
}
.location-modal .modal-body,
.location-modal .modal-header,
.location-modal .modal-content {
    float: left;
    width: 100%;
}
.location-modal .modal-content {
    margin-left: 13px;
}
.location-modal .modal-body {
    padding: 0;
}
.location-modal .modal-header {
    border-bottom: 0px;
}
.location-modal .modal-header button.close {
    margin-top: 8px;
    font-size: 36px;
}
.location-modal-search {
    background: #8156a3;
    padding: 8px 16px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
.location-modal-search input {
    padding: 0px 12px 0px 30px;
    background: url(../img/sprite-hm.png) no-repeat 8px -68px;
    background-color: #fff;
    border-color: #fff;
}
.location-modal-search input:focus {
    box-shadow: none;
}
.popular-cities-box {
    padding: 15px;
    background: #f3f3f3;
    border-bottom: 1px solid #cccccc;
}
.popular-cities-box h4 {
    font-size: 11px;
    line-height: 16px;
}
.popular-cities-box ul li {
    display: inline-block;
    width: 24%;
    font-size: 11px;
    line-height: 16px;
}
.popular-cities-box ul li a:hover {
